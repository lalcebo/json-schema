{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Amazon DataZone projects are business use caseâ€“based groupings of people, assets (data), and tools used to simplify access to the AWS analytics.",
  "definitions": {},
  "properties": {
    "CreatedAt": {
      "oneOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The timestamp of when the project was created."
    },
    "CreatedBy": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The Amazon DataZone user who created the project."
    },
    "Description": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The description of the Amazon DataZone project."
    },
    "DomainId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^dzd[-_][a-zA-Z0-9_-]{1,36}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The identifier of the Amazon DataZone domain in which the project was created."
    },
    "DomainIdentifier": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^dzd[-_][a-zA-Z0-9_-]{1,36}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The ID of the Amazon DataZone domain in which this project is created."
    },
    "GlossaryTerms": {
      "description": "The glossary terms that can be used in this Amazon DataZone project.",
      "type": "array",
      "insertionOrder": false,
      "items": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_-]{1,36}$"
      },
      "maxItems": 20,
      "minItems": 1
    },
    "Id": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[a-zA-Z0-9_-]{1,36}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The ID of the Amazon DataZone project."
    },
    "LastUpdatedAt": {
      "oneOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The timestamp of when the project was last updated."
    },
    "Name": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 64,
          "minLength": 1,
          "pattern": "^[\\w -]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The name of the Amazon DataZone project."
    }
  },
  "required": [
    "DomainIdentifier",
    "Name"
  ],
  "additionalIdentifiers": [
    [
      "/properties/DomainIdentifier"
    ]
  ],
  "additionalProperties": false
}