{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource Type definition for AWS::Events::ApiDestination.",
  "properties": {
    "Name": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 64
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Name of the apiDestination."
    },
    "Description": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 512
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ConnectionArn": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The arn of the connection."
    },
    "Arn": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The arn of the api destination."
    },
    "InvocationRateLimitPerSecond": {
      "type": "integer",
      "minimum": 1
    },
    "InvocationEndpoint": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Url endpoint to invoke."
    },
    "HttpMethod": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "GET",
            "HEAD",
            "POST",
            "OPTIONS",
            "PUT",
            "DELETE",
            "PATCH"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "ConnectionArn",
    "InvocationEndpoint",
    "HttpMethod"
  ]
}