{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource Type definition for AWS::Transfer::Certificate",
  "definitions": {
    "Tag": {
      "description": "A key-value pair to associate with a resource.",
      "type": "object",
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "The key name of the tag. You can specify a value that is 1 to 128 Unicode characters in length and cannot be prefixed with aws:. You can use any of the following characters: the set of Unicode letters, digits, whitespace, _, ., /, =, +, and -."
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 0,
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "The value for the tag. You can specify a value that is 0 to 256 Unicode characters in length and cannot be prefixed with aws:. You can use any of the following characters: the set of Unicode letters, digits, whitespace, _, ., /, =, +, and -."
        }
      },
      "required": [
        "Key",
        "Value"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "Usage": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "SIGNING",
            "ENCRYPTION"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the usage type for the certificate."
    },
    "Certificate": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[\t\n\r -ÿ]*",
          "minLength": 1,
          "maxLength": 16384
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the certificate body to be imported."
    },
    "CertificateChain": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[\t\n\r -ÿ]*",
          "minLength": 1,
          "maxLength": 2097152
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the certificate chain to be imported."
    },
    "PrivateKey": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[\t\n\r -ÿ]*",
          "minLength": 1,
          "maxLength": 16384
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the private key for the certificate."
    },
    "ActiveDate": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the active date for the certificate."
    },
    "InactiveDate": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the inactive date for the certificate."
    },
    "Description": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[\\w\\- ]*$",
          "minLength": 1,
          "maxLength": 200
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A textual description for the certificate."
    },
    "Tags": {
      "description": "Key-value pairs that can be used to group and search for certificates. Tags are metadata attached to certificates for any purpose.",
      "type": "array",
      "maxItems": 50,
      "uniqueItems": true,
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      }
    },
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "arn:.*",
          "minLength": 20,
          "maxLength": 1600
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the unique Amazon Resource Name (ARN) for the agreement."
    },
    "CertificateId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^cert-([0-9a-f]{17})$",
          "minLength": 22,
          "maxLength": 22
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A unique identifier for the certificate."
    },
    "Status": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "ACTIVE",
            "PENDING",
            "INACTIVE"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A status description for the certificate."
    },
    "Type": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "CERTIFICATE",
            "CERTIFICATE_WITH_PRIVATE_KEY"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Describing the type of certificate. With or without a private key."
    },
    "Serial": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[\\p{XDigit}{2}:?]*",
          "minLength": 0,
          "maxLength": 48
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies Certificate's serial."
    },
    "NotBeforeDate": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the not before date for the certificate."
    },
    "NotAfterDate": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the not after date for the certificate."
    }
  },
  "additionalProperties": false,
  "required": [
    "Certificate",
    "Usage"
  ]
}