{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "The AWS::VoiceID::Domain resource specifies an Amazon VoiceID Domain.",
  "definitions": {
    "ServerSideEncryptionConfiguration": {
      "type": "object",
      "properties": {
        "KmsKeyId": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 2048,
              "minLength": 1
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "KmsKeyId"
      ],
      "additionalProperties": false
    },
    "Tag": {
      "type": "object",
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 128,
              "minLength": 1,
              "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 256,
              "minLength": 0,
              "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Key",
        "Value"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "Description": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 1024,
          "minLength": 1,
          "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-%@]*)$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "DomainId": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 22,
          "minLength": 22,
          "pattern": "^[a-zA-Z0-9]{22}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Name": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 256,
          "minLength": 1,
          "pattern": "^[a-zA-Z0-9][a-zA-Z0-9_-]*$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServerSideEncryptionConfiguration": {
      "$ref": "#/definitions/ServerSideEncryptionConfiguration"
    },
    "Tags": {
      "type": "array",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "maxItems": 200,
      "minItems": 0
    }
  },
  "required": [
    "Name",
    "ServerSideEncryptionConfiguration"
  ],
  "additionalProperties": false
}