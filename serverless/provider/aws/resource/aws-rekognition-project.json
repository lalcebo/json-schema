{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "The AWS::Rekognition::Project type creates an Amazon Rekognition CustomLabels Project. A project is a grouping of the resources needed to create and manage Dataset and ProjectVersions.",
  "definitions": {
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "pattern": "(^arn:[a-z\\d-]+:rekognition:[a-z\\d-]+:\\d{12}:project/[a-zA-Z0-9_.\\-]{1,255}/[0-9]+$)"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ProjectName": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 255,
          "pattern": "[a-zA-Z0-9][a-zA-Z0-9_\\-]*"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The name of the project"
    }
  },
  "properties": {
    "Arn": {
      "$ref": "#/definitions/Arn"
    },
    "ProjectName": {
      "$ref": "#/definitions/ProjectName"
    }
  },
  "additionalProperties": false,
  "required": [
    "ProjectName"
  ]
}