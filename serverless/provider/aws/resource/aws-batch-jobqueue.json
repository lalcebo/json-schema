{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource Type definition for AWS::Batch::JobQueue",
  "additionalProperties": false,
  "definitions": {
    "ResourceArn": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "arn:[a-z0-9-\\.]{1,63}:[a-z0-9-\\.]{0,63}:[a-z0-9-\\.]{0,63}:[a-z0-9-\\.]{0,63}:[^/].{0,1023}"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ComputeEnvironmentOrder": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ComputeEnvironment": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Order": {
          "type": "integer"
        }
      },
      "required": [
        "ComputeEnvironment",
        "Order"
      ]
    }
  },
  "properties": {
    "JobQueueName": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 128
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "JobQueueArn": {
      "$ref": "#/definitions/ResourceArn"
    },
    "ComputeEnvironmentOrder": {
      "type": "array",
      "insertionOrder": true,
      "uniqueItems": false,
      "items": {
        "$ref": "#/definitions/ComputeEnvironmentOrder"
      }
    },
    "Priority": {
      "type": "integer",
      "minimum": 0,
      "maximum": 1000
    },
    "State": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "DISABLED",
            "ENABLED"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "SchedulingPolicyArn": {
      "$ref": "#/definitions/ResourceArn"
    },
    "Tags": {
      "type": "object",
      "description": "A key-value pair to associate with a resource.",
      "patternProperties": {
        ".*": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  },
  "required": [
    "ComputeEnvironmentOrder",
    "Priority"
  ]
}