          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "SseConfig": {
      "type": "object",
      "properties": {
        "Type": {
          "$ref": "#/definitions/EncryptionType"
        },
        "KeyArn": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 2048,
              "minLength": 20,
              "pattern": "arn:([^:\n]*):([^:\n]*):([^:\n]*):([0-9]{12}):([^:\n]*)"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Type"
      ],
      "additionalProperties": false
