        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Reference": {
      "$ref": "#/definitions/ReferenceItem"
    },
    "SseConfig": {
      "$ref": "#/definitions/SseConfig"
    },
    "Status": {
      "$ref": "#/definitions/StoreStatus"
    },
    "StatusMessage": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 1000,
          "minLength": 0
