            }
          },
          "required": [
            "TsvStoreOptions"
          ],
          "additionalProperties": false
        }
      ]
    },
    "StoreStatus": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "CREATING",
            "UPDATING",
            "DELETING",
            "ACTIVE",
            "FAILED"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "TagMap": {
      "type": "object",
      "patternProperties": {
        ".+": {
          "type": "string",
          "maxLength": 256,
          "minLength": 0
