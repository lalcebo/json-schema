    "EncryptionType": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "KMS"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "FormatToHeader": {
      "type": "object",
      "patternProperties": {
        ".+": {
          "type": "string",
          "maxLength": 1000,
          "minLength": 0
        }
      },
      "additionalProperties": false
