{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "A service network is a logical boundary for a collection of services. You can associate services and VPCs with a service network.",
  "additionalProperties": false,
  "definitions": {
    "Tag": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Key",
        "Value"
      ]
    }
  },
  "properties": {
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^arn:[a-z0-9\\-]+:vpc-lattice:[a-zA-Z0-9\\-]+:\\d{12}:servicenetwork/sn-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CreatedAt": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Id": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 20,
          "minLength": 20,
          "pattern": "^sn-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "LastUpdatedAt": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Name": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 63,
          "minLength": 3,
          "pattern": "^(?!servicenetwork-)(?![-])(?!.*[-]$)(?!.*[-]{2})[a-z0-9-]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "AuthType": {
      "oneOf": [
        {
          "type": "string",
          "default": "NONE",
          "enum": [
            "NONE",
            "AWS_IAM"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Tags": {
      "type": "array",
      "insertionOrder": false,
      "uniqueItems": true,
      "minItems": 0,
      "maxItems": 50,
      "items": {
        "$ref": "#/definitions/Tag"
      }
    }
  },
  "additionalIdentifiers": [
    [
      "/properties/Id"
    ],
    [
      "/properties/Name"
    ]
  ]
}