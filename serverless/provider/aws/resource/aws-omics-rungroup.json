{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Definition of AWS::Omics::RunGroup Resource Type",
  "definitions": {
    "TagMap": {
      "type": "object",
      "description": "A map of resource tags",
      "patternProperties": {
        ".+": {
          "type": "string",
          "maxLength": 256,
          "minLength": 0,
          "description": "Resource tag value"
        }
      },
      "additionalProperties": false
    }
  },
  "properties": {
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 128,
          "minLength": 1,
          "pattern": "^arn:.+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CreationTime": {
      "oneOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Id": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 18,
          "minLength": 1,
          "pattern": "^[0-9]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "MaxCpus": {
      "type": "number",
      "maximum": 100000,
      "minimum": 1
    },
    "MaxGpus": {
      "type": "number",
      "maximum": 100000,
      "minimum": 1
    },
    "MaxDuration": {
      "type": "number",
      "maximum": 100000,
      "minimum": 1
    },
    "MaxRuns": {
      "type": "number",
      "maximum": 100000,
      "minimum": 1
    },
    "Name": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 128,
          "minLength": 1,
          "pattern": "^[\\p{L}||\\p{M}||\\p{Z}||\\p{S}||\\p{N}||\\p{P}]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Tags": {
      "$ref": "#/definitions/TagMap"
    }
  },
  "additionalProperties": false
}