{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource schema for AWS::DataBrew::Project.",
  "taggable": true,
  "properties": {
    "DatasetName": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Dataset name"
    },
    "Name": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Project name"
    },
    "RecipeName": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Recipe name"
    },
    "RoleArn": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Role arn"
    },
    "Sample": {
      "description": "Sample",
      "$ref": "#/definitions/Sample"
    },
    "Tags": {
      "type": "array",
      "insertionOrder": false,
      "uniqueItems": false,
      "items": {
        "$ref": "#/definitions/Tag"
      }
    }
  },
  "definitions": {
    "Sample": {
      "type": "object",
      "properties": {
        "Size": {
          "description": "Sample size",
          "minimum": 1,
          "type": "integer"
        },
        "Type": {
          "oneOf": [
            {
              "enum": [
                "FIRST_N",
                "LAST_N",
                "RANDOM"
              ],
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "Sample type"
        }
      },
      "additionalProperties": false,
      "required": [
        "Type"
      ]
    },
    "Tag": {
      "description": "A key-value pair to associate with a resource.",
      "type": "object",
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 0,
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "Value",
        "Key"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "DatasetName",
    "Name",
    "RecipeName",
    "RoleArn"
  ]
}