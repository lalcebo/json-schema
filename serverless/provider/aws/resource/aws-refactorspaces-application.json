{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Definition of AWS::RefactorSpaces::Application Resource Type",
  "definitions": {
    "ApiGatewayEndpointType": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "REGIONAL",
            "PRIVATE"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ApiGatewayProxyInput": {
      "type": "object",
      "properties": {
        "StageName": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 128,
              "minLength": 1,
              "pattern": "^[-a-zA-Z0-9_]*$"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "EndpointType": {
          "$ref": "#/definitions/ApiGatewayEndpointType"
        }
      },
      "additionalProperties": false
    },
    "ProxyType": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "API_GATEWAY"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Tag": {
      "description": "A label for tagging Environment resource",
      "type": "object",
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128,
              "pattern": "^(?!aws:).+"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "A string used to identify this tag"
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 0,
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "A string containing the value for the tag"
        }
      },
      "required": [
        "Key",
        "Value"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "ApiGatewayProxy": {
      "$ref": "#/definitions/ApiGatewayProxyInput"
    },
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^arn:(aws[a-zA-Z-]*)?:refactor-spaces:[a-zA-Z0-9\\-]+:\\w{12}:[a-zA-Z_0-9+=,.@\\-_/]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ApiGatewayId": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 10,
          "minLength": 10,
          "pattern": "^[a-z0-9]{10}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "VpcLinkId": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 10,
          "minLength": 10,
          "pattern": "^[a-z0-9]{10}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "NlbArn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^arn:(aws[a-zA-Z-]*)?:elasticloadbalancing:[a-zA-Z0-9\\\\-]+:\\\\w{12}:[a-zA-Z_0-9+=,.@\\\\-_\\/]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "NlbName": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 32,
          "minLength": 1,
          "pattern": "^(?!internal-)[a-zA-Z0-9]+[a-zA-Z0-9-_ ]+.*[^-]$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ApplicationIdentifier": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 14,
          "minLength": 14,
          "pattern": "^app-([0-9A-Za-z]{10}$)"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "EnvironmentIdentifier": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 14,
          "minLength": 14,
          "pattern": "^env-([0-9A-Za-z]{10}$)"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Name": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 63,
          "minLength": 3,
          "pattern": "^(?!app-)[a-zA-Z0-9]+[a-zA-Z0-9-_ ]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ProxyType": {
      "$ref": "#/definitions/ProxyType"
    },
    "VpcId": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 21,
          "minLength": 12,
          "pattern": "^vpc-[-a-f0-9]{8}([-a-f0-9]{9})?$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "StageName": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 128,
          "minLength": 1,
          "pattern": "^[-a-zA-Z0-9_]*$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ProxyUrl": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 1,
          "pattern": "^http://[-a-zA-Z0-9+\\x38@#/%?=~_|!:,.;]*[-a-zA-Z0-9+\\x38@#/%=~_|]$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Tags": {
      "type": "array",
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "description": "Metadata that you can assign to help organize the frameworks that you create. Each tag is a key-value pair."
    }
  },
  "required": [
    "EnvironmentIdentifier",
    "VpcId",
    "Name",
    "ProxyType"
  ],
  "additionalProperties": false,
  "taggable": true
}