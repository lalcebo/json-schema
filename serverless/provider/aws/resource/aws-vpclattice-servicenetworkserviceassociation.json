{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Associates a service with a service network.",
  "additionalProperties": false,
  "definitions": {
    "DnsEntry": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "DomainName": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "HostedZoneId": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      }
    },
    "Tag": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Key",
        "Value"
      ]
    }
  },
  "properties": {
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^arn:[a-z0-9\\-]+:vpc-lattice:[a-zA-Z0-9\\-]+:\\d{12}:servicenetworkserviceassociation/snsa-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CreatedAt": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "DnsEntry": {
      "$ref": "#/definitions/DnsEntry"
    },
    "Id": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 17,
          "pattern": "^snsa-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceNetworkArn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^arn:[a-z0-9\\-]+:vpc-lattice:[a-zA-Z0-9\\-]+:\\d{12}:servicenetwork/sn-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceNetworkId": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 20,
          "minLength": 20,
          "pattern": "^sn-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceNetworkIdentifier": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^((sn-[0-9a-z]{17})|(arn:[a-z0-9\\-]+:vpc-lattice:[a-zA-Z0-9\\-]+:\\d{12}:servicenetwork/sn-[0-9a-z]{17}))$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceNetworkName": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 63,
          "minLength": 3,
          "pattern": "^(?!servicenetwork-)(?![-])(?!.*[-]$)(?!.*[-]{2})[a-z0-9-]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceArn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^arn:[a-z0-9\\-]+:vpc-lattice:[a-zA-Z0-9\\-]+:\\d{12}:service/svc-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceId": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 21,
          "minLength": 21,
          "pattern": "^svc-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceIdentifier": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^((svc-[0-9a-z]{17})|(arn:[a-z0-9\\-]+:vpc-lattice:[a-zA-Z0-9\\-]+:\\d{12}:service/svc-[0-9a-z]{17}))$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceName": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 40,
          "minLength": 3,
          "pattern": "^(?!svc-)(?![-])(?!.*[-]$)(?!.*[-]{2})[a-z0-9-]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Status": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "CREATE_IN_PROGRESS",
            "ACTIVE",
            "DELETE_IN_PROGRESS",
            "CREATE_FAILED",
            "DELETE_FAILED"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Tags": {
      "type": "array",
      "insertionOrder": false,
      "uniqueItems": true,
      "minItems": 0,
      "maxItems": 50,
      "items": {
        "$ref": "#/definitions/Tag"
      }
    }
  },
  "additionalIdentifiers": [
    [
      "/properties/Id"
    ],
    [
      "/properties/ServiceNetworkIdentifier",
      "/properties/ServiceIdentifier"
    ]
  ]
}