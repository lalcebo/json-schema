{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource schema for AWS::Athena::PreparedStatement",
  "properties": {
    "StatementName": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 256
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The name of the prepared statement."
    },
    "WorkGroup": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 128
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The name of the workgroup to which the prepared statement belongs."
    },
    "Description": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 1024
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The description of the prepared statement."
    },
    "QueryStatement": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 262144
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The query string for the prepared statement."
    }
  },
  "required": [
    "StatementName",
    "WorkGroup",
    "QueryStatement"
  ],
  "additionalProperties": false,
  "taggable": false
}