{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Definition of AWS::PCAConnectorAD::TemplateGroupAccessControlEntry Resource Type",
  "definitions": {
    "AccessRight": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "ALLOW",
            "DENY"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "AccessRights": {
      "type": "object",
      "properties": {
        "Enroll": {
          "$ref": "#/definitions/AccessRight"
        },
        "AutoEnroll": {
          "$ref": "#/definitions/AccessRight"
        }
      },
      "additionalProperties": false
    }
  },
  "properties": {
    "AccessRights": {
      "$ref": "#/definitions/AccessRights"
    },
    "GroupDisplayName": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 256,
          "minLength": 0,
          "pattern": "^[\\x20-\\x7E]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "GroupSecurityIdentifier": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 256,
          "minLength": 7,
          "pattern": "^S-[0-9]-([0-9]+-){1,14}[0-9]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "TemplateArn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 200,
          "minLength": 5,
          "pattern": "^arn:[\\w-]+:pca-connector-ad:[\\w-]+:[0-9]+:connector(\\/[\\w-]+)\\/template(\\/[\\w-]+)$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    }
  },
  "required": [
    "AccessRights",
    "GroupDisplayName"
  ],
  "additionalProperties": false
}