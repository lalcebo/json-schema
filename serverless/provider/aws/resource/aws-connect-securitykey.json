{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource Type definition for AWS::Connect::SecurityKey",
  "definitions": {
    "Key": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 1024
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A valid security key in PEM format."
    },
    "InstanceId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^arn:aws[-a-z0-9]*:connect:[-a-z0-9]*:[0-9]{12}:instance/[-a-zA-Z0-9]*$",
          "minLength": 1,
          "maxLength": 100
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Amazon Connect instance identifier"
    },
    "AssociationId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[-a-z0-9]*$",
          "minLength": 1,
          "maxLength": 100
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "An associationID is automatically generated when a storage config is associated with an instance"
    }
  },
  "properties": {
    "Key": {
      "$ref": "#/definitions/Key"
    },
    "InstanceId": {
      "$ref": "#/definitions/InstanceId"
    },
    "AssociationId": {
      "$ref": "#/definitions/AssociationId"
    }
  },
  "additionalProperties": false,
  "required": [
    "Key",
    "InstanceId"
  ]
}