{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource Type definition for AWS::OpsWorksCM::Server",
  "additionalProperties": false,
  "properties": {
    "KeyPair": {
      "oneOf": [
        {
          "type": "string",
          "pattern": ".*",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "EngineVersion": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServiceRoleArn": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "arn:aws:iam::[0-9]{12}:role/.*",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "DisableAutomatedBackup": {
      "type": "boolean"
    },
    "BackupId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "[a-zA-Z][a-zA-Z0-9\\-\\.\\:]*",
          "maxLength": 79
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "EngineModel": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "PreferredMaintenanceWindow": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^((Mon|Tue|Wed|Thu|Fri|Sat|Sun):)?([0-1][0-9]|2[0-3]):[0-5][0-9]$",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "AssociatePublicIpAddress": {
      "type": "boolean"
    },
    "InstanceProfileArn": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "arn:aws:iam::[0-9]{12}:instance-profile/.*",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CustomCertificate": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "(?s)\\s*-----BEGIN CERTIFICATE-----.+-----END CERTIFICATE-----\\s*",
          "maxLength": 2097152
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "PreferredBackupWindow": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^((Mon|Tue|Wed|Thu|Fri|Sat|Sun):)?([0-1][0-9]|2[0-3]):[0-5][0-9]$",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "SecurityGroupIds": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string",
        "maxLength": 10000
      }
    },
    "SubnetIds": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string",
        "maxLength": 10000
      }
    },
    "CustomDomain": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^(((?!-)[A-Za-z0-9-]{0,62}[A-Za-z0-9])\\.)+((?!-)[A-Za-z0-9-]{1,62}[A-Za-z0-9])$",
          "maxLength": 253
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Endpoint": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CustomPrivateKey": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "(?ms)\\s*^-----BEGIN (?-s:.*)PRIVATE KEY-----$.*?^-----END (?-s:.*)PRIVATE KEY-----$\\s*",
          "maxLength": 4096
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ServerName": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 40,
          "pattern": "[a-zA-Z][a-zA-Z0-9\\-]*"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "EngineAttributes": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "$ref": "#/definitions/EngineAttribute"
      }
    },
    "BackupRetentionCount": {
      "type": "integer",
      "minLength": 1
    },
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "InstanceType": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Tags": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "$ref": "#/definitions/Tag"
      }
    },
    "Engine": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 10000
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    }
  },
  "definitions": {
    "EngineAttribute": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "(?s).*",
              "maxLength": 10000
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Name": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "(?s).*",
              "maxLength": 10000
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      }
    },
    "Tag": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
              "minLength": 0,
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Value",
        "Key"
      ]
    }
  },
  "required": [
    "ServiceRoleArn",
    "InstanceProfileArn",
    "InstanceType"
  ]
}