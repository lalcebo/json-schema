{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "A service is any software application that can run on instances containers, or serverless functions within an account or virtual private cloud (VPC).",
  "additionalProperties": false,
  "definitions": {
    "DnsEntry": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "DomainName": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "HostedZoneId": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      }
    },
    "Tag": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Key",
        "Value"
      ]
    }
  },
  "properties": {
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "minLength": 20,
          "pattern": "^arn:[a-z0-9\\-]+:vpc-lattice:[a-zA-Z0-9\\-]+:\\d{12}:service/svc-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "AuthType": {
      "oneOf": [
        {
          "type": "string",
          "default": "NONE",
          "enum": [
            "NONE",
            "AWS_IAM"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CreatedAt": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "DnsEntry": {
      "$ref": "#/definitions/DnsEntry"
    },
    "Id": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 21,
          "minLength": 21,
          "pattern": "^svc-[0-9a-z]{17}$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "LastUpdatedAt": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Name": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 40,
          "minLength": 3,
          "pattern": "^(?!svc-)(?![-])(?!.*[-]$)(?!.*[-]{2})[a-z0-9-]+$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Status": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "ACTIVE",
            "CREATE_IN_PROGRESS",
            "DELETE_IN_PROGRESS",
            "CREATE_FAILED",
            "DELETE_FAILED"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CertificateArn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048,
          "pattern": "^(arn(:[a-z0-9]+([.-][a-z0-9]+)*){2}(:([a-z0-9]+([.-][a-z0-9]+)*)?){2}:certificate/[0-9a-z-]+)?$"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CustomDomainName": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 255,
          "minLength": 3
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Tags": {
      "type": "array",
      "insertionOrder": false,
      "uniqueItems": true,
      "minItems": 0,
      "maxItems": 50,
      "items": {
        "$ref": "#/definitions/Tag"
      }
    }
  },
  "additionalIdentifiers": [
    [
      "/properties/Id"
    ],
    [
      "/properties/Name"
    ]
  ]
}