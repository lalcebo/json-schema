{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Definition of AWS::DataZone::EnvironmentBlueprintConfiguration Resource Type",
  "required": [
    "DomainIdentifier",
    "EnvironmentBlueprintIdentifier",
    "EnabledRegions"
  ],
  "additionalIdentifiers": [
    [
      "/properties/DomainIdentifier",
      "/properties/EnvironmentBlueprintIdentifier"
    ]
  ],
  "additionalProperties": false,
  "definitions": {
    "RegionalParameter": {
      "additionalProperties": false,
      "type": "object",
      "properties": {
        "Parameters": {
          "$ref": "#/definitions/Parameter"
        },
        "Region": {
          "oneOf": [
            {
              "pattern": "^[a-z]{2}-?(iso|gov)?-{1}[a-z]*-{1}[0-9]$",
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      }
    },
    "Parameter": {
      "patternProperties": {
        ".+": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    }
  },
  "properties": {
    "RegionalParameters": {
      "uniqueItems": true,
      "insertionOrder": false,
      "type": "array",
      "items": {
        "$ref": "#/definitions/RegionalParameter"
      }
    },
    "ProvisioningRoleArn": {
      "oneOf": [
        {
          "pattern": "^arn:aws[^:]*:iam::\\d{12}:(role|role/service-role)/[\\w+=,.@-]*$",
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "DomainId": {
      "oneOf": [
        {
          "pattern": "^dzd[-_][a-zA-Z0-9_-]{1,36}$",
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CreatedAt": {
      "oneOf": [
        {
          "format": "date-time",
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "EnabledRegions": {
      "minItems": 0,
      "insertionOrder": false,
      "type": "array",
      "items": {
        "minLength": 4,
        "pattern": "^[a-z]{2}-?(iso|gov)?-{1}[a-z]*-{1}[0-9]$",
        "type": "string",
        "maxLength": 16
      }
    },
    "EnvironmentBlueprintIdentifier": {
      "oneOf": [
        {
          "pattern": "^[a-zA-Z0-9_-]{1,36}$",
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "EnvironmentBlueprintId": {
      "oneOf": [
        {
          "pattern": "^[a-zA-Z0-9_-]{1,36}$",
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "UpdatedAt": {
      "oneOf": [
        {
          "format": "date-time",
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "DomainIdentifier": {
      "oneOf": [
        {
          "pattern": "^dzd[-_][a-zA-Z0-9_-]{1,36}$",
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ManageAccessRoleArn": {
      "oneOf": [
        {
          "pattern": "^arn:aws[^:]*:iam::\\d{12}:(role|role/service-role)/[\\w+=,.@-]*$",
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    }
  }
}