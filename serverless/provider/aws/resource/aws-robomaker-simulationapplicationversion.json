{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "AWS::RoboMaker::SimulationApplicationVersion resource creates an AWS RoboMaker SimulationApplicationVersion. This helps you control which code your simulation uses.",
  "definitions": {
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "arn:[\\w+=/,.@-]+:[\\w+=/,.@-]+:[\\w+=/,.@-]*:[0-9]*:[\\w+=,.@-]+(/[\\w+=,.@-]+)*"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    }
  },
  "properties": {
    "Application": {
      "$ref": "#/definitions/Arn"
    },
    "CurrentRevisionId": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 40,
          "pattern": "[a-zA-Z0-9_.\\-]*"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The revision ID of robot application."
    },
    "ApplicationVersion": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Arn": {
      "$ref": "#/definitions/Arn"
    }
  },
  "additionalProperties": false,
  "required": [
    "Application"
  ],
  "taggable": false
}