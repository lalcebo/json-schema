{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource Type definition for AWS::Transfer::Agreement",
  "definitions": {
    "Tag": {
      "description": "Creates a key-value pair for a specific resource.",
      "type": "object",
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "The name assigned to the tag that you create."
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 0,
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "Contains one or more values that you assigned to the key name you create."
        }
      },
      "required": [
        "Key",
        "Value"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "Description": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^[\\w\\- ]*$",
          "minLength": 1,
          "maxLength": 200
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A textual description for the agreement."
    },
    "ServerId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^s-([0-9a-f]{17})$",
          "minLength": 19,
          "maxLength": 19
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A unique identifier for the server."
    },
    "LocalProfileId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^p-([0-9a-f]{17})$",
          "minLength": 19,
          "maxLength": 19
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A unique identifier for the local profile."
    },
    "PartnerProfileId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^p-([0-9a-f]{17})$",
          "minLength": 19,
          "maxLength": 19
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A unique identifier for the partner profile."
    },
    "BaseDirectory": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^$|/.*",
          "maxLength": 1024
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the base directory for the agreement."
    },
    "AccessRole": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "arn:.*role/.*",
          "minLength": 20,
          "maxLength": 2048
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the access role for the agreement."
    },
    "Status": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "ACTIVE",
            "INACTIVE"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the status of the agreement."
    },
    "Tags": {
      "description": "Key-value pairs that can be used to group and search for agreements. Tags are metadata attached to agreements for any purpose.",
      "type": "array",
      "maxItems": 50,
      "uniqueItems": true,
      "insertionOrder": false,
      "items": {
        "$ref": "#/definitions/Tag"
      }
    },
    "AgreementId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "^a-([0-9a-f]{17})$",
          "minLength": 19,
          "maxLength": 19
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "A unique identifier for the agreement."
    },
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "arn:.*",
          "minLength": 20,
          "maxLength": 1600
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Specifies the unique Amazon Resource Name (ARN) for the agreement."
    }
  },
  "additionalProperties": false,
  "required": [
    "ServerId",
    "LocalProfileId",
    "PartnerProfileId",
    "BaseDirectory",
    "AccessRole"
  ]
}