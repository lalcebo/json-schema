{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "The AWS::Logs::LogAnomalyDetector resource specifies a CloudWatch Logs LogAnomalyDetector.",
  "properties": {
    "AccountId": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Account ID for owner of detector"
    },
    "KmsKeyId": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 256
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The Amazon Resource Name (ARN) of the CMK to use when encrypting log data."
    },
    "DetectorName": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Name of detector"
    },
    "LogGroupArnList": {
      "description": "List of Arns for the given log group",
      "type": "array",
      "uniqueItems": true,
      "insertionOrder": false,
      "items": {
        "type": "string",
        "minLength": 20,
        "maxLength": 2048
      }
    },
    "EvaluationFrequency": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "FIVE_MIN",
            "TEN_MIN",
            "FIFTEEN_MIN",
            "THIRTY_MIN",
            "ONE_HOUR"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "How often log group is evaluated"
    },
    "FilterPattern": {
      "oneOf": [
        {
          "description": "",
          "type": "string",
          "pattern": ""
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "AnomalyDetectorStatus": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Current status of detector."
    },
    "AnomalyVisibilityTime": {
      "description": "",
      "type": "number"
    },
    "CreationTimeStamp": {
      "description": "When detector was created.",
      "type": "number"
    },
    "LastModifiedTimeStamp": {
      "description": "When detector was lsat modified.",
      "type": "number"
    },
    "AnomalyDetectorArn": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "ARN of LogAnomalyDetector"
    }
  },
  "additionalProperties": false,
  "required": []
}