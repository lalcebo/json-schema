{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource Type definition for AWS::LicenseManager::License",
  "definitions": {
    "ValidityDateFormat": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Begin": {
          "oneOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "Validity begin date for the license."
        },
        "End": {
          "oneOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ],
          "description": "Validity begin date for the license."
        }
      },
      "required": [
        "Begin",
        "End"
      ]
    },
    "IssuerData": {
      "type": "object",
      "properties": {
        "Name": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "SignKey": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Name"
      ],
      "additionalProperties": false
    },
    "Entitlement": {
      "type": "object",
      "properties": {
        "Name": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "MaxCount": {
          "type": "integer"
        },
        "Overage": {
          "type": "boolean"
        },
        "Unit": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "AllowCheckIn": {
          "type": "boolean"
        }
      },
      "required": [
        "Name",
        "Unit"
      ],
      "additionalProperties": false
    },
    "ConsumptionConfiguration": {
      "type": "object",
      "properties": {
        "RenewType": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "ProvisionalConfiguration": {
          "$ref": "#/definitions/ProvisionalConfiguration"
        },
        "BorrowConfiguration": {
          "$ref": "#/definitions/BorrowConfiguration"
        }
      },
      "additionalProperties": false
    },
    "ProvisionalConfiguration": {
      "type": "object",
      "properties": {
        "MaxTimeToLiveInMinutes": {
          "type": "integer"
        }
      },
      "required": [
        "MaxTimeToLiveInMinutes"
      ],
      "additionalProperties": false
    },
    "BorrowConfiguration": {
      "type": "object",
      "properties": {
        "MaxTimeToLiveInMinutes": {
          "type": "integer"
        },
        "AllowEarlyCheckIn": {
          "type": "boolean"
        }
      },
      "required": [
        "MaxTimeToLiveInMinutes",
        "AllowEarlyCheckIn"
      ],
      "additionalProperties": false
    },
    "Metadata": {
      "type": "object",
      "properties": {
        "Name": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Name",
        "Value"
      ],
      "additionalProperties": false
    },
    "LicenseStatus": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Arn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 2048
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    }
  },
  "properties": {
    "ProductSKU": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 1024
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "ProductSKU of the license."
    },
    "Issuer": {
      "$ref": "#/definitions/IssuerData"
    },
    "LicenseName": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Name for the created license."
    },
    "ProductName": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Product name for the created license."
    },
    "HomeRegion": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Home region for the created license."
    },
    "Validity": {
      "$ref": "#/definitions/ValidityDateFormat"
    },
    "Entitlements": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "$ref": "#/definitions/Entitlement"
      }
    },
    "Beneficiary": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Beneficiary of the license."
    },
    "ConsumptionConfiguration": {
      "$ref": "#/definitions/ConsumptionConfiguration"
    },
    "LicenseMetadata": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "$ref": "#/definitions/Metadata"
      }
    },
    "LicenseArn": {
      "description": "Amazon Resource Name is a unique name for each resource.",
      "$ref": "#/definitions/Arn"
    },
    "Status": {
      "$ref": "#/definitions/LicenseStatus"
    },
    "Version": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The version of the license."
    }
  },
  "additionalProperties": false,
  "required": [
    "LicenseName",
    "ProductName",
    "Issuer",
    "HomeRegion",
    "Validity",
    "ConsumptionConfiguration",
    "Entitlements"
  ]
}