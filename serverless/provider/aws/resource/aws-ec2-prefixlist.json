{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Resource schema of AWS::EC2::PrefixList Type",
  "definitions": {
    "Tag": {
      "type": "object",
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 256
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Key"
      ],
      "additionalProperties": false
    },
    "Entry": {
      "type": "object",
      "properties": {
        "Cidr": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 46
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Description": {
          "oneOf": [
            {
              "type": "string",
              "minLength": 0,
              "maxLength": 255
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Cidr"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "PrefixListName": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Name of Prefix List."
    },
    "PrefixListId": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Id of Prefix List."
    },
    "OwnerId": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Owner Id of Prefix List."
    },
    "AddressFamily": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "IPv4",
            "IPv6"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Ip Version of Prefix List."
    },
    "MaxEntries": {
      "description": "Max Entries of Prefix List.",
      "type": "integer",
      "minimum": 1
    },
    "Version": {
      "description": "Version of Prefix List.",
      "type": "integer"
    },
    "Tags": {
      "description": "Tags for Prefix List",
      "type": "array",
      "items": {
        "$ref": "#/definitions/Tag"
      }
    },
    "Entries": {
      "description": "Entries of Prefix List.",
      "type": "array",
      "items": {
        "$ref": "#/definitions/Entry"
      }
    },
    "Arn": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The Amazon Resource Name (ARN) of the Prefix List."
    }
  },
  "required": [
    "PrefixListName",
    "AddressFamily"
  ],
  "additionalProperties": false
}