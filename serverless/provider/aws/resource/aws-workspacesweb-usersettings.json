        {
          "type": "string",
          "enum": [
            "Disabled",
            "Enabled"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "EncryptionContextMap": {
      "type": "object",
      "patternProperties": {
        "^[\\s\\S]*$": {
          "type": "string",
          "maxLength": 131072,
          "minLength": 0,
          "pattern": "^[\\s\\S]*$"
