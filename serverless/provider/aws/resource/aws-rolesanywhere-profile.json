{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Definition of AWS::RolesAnywhere::Profile Resource Type",
  "definitions": {
    "Tag": {
      "type": "object",
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 128,
              "minLength": 1
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string",
              "maxLength": 256,
              "minLength": 0
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Key",
        "Value"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "DurationSeconds": {
      "type": "number",
      "maximum": 43200,
      "minimum": 900
    },
    "Enabled": {
      "type": "boolean"
    },
    "ManagedPolicyArns": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "Name": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ProfileArn": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "ProfileId": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "[a-f0-9]{8}-([a-z0-9]{4}-){3}[a-z0-9]{12}"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "RequireInstanceProperties": {
      "type": "boolean"
    },
    "RoleArns": {
      "type": "array",
      "items": {
        "type": "string",
        "maxLength": 1011,
        "minLength": 1
      }
    },
    "SessionPolicy": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Tags": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "maxItems": 200,
      "minItems": 0
    }
  },
  "required": [
    "Name",
    "RoleArns"
  ],
  "additionalProperties": false
}