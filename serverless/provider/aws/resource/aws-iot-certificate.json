{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Use the AWS::IoT::Certificate resource to declare an AWS IoT X.509 certificate.",
  "definitions": {},
  "properties": {
    "CACertificatePem": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 65536
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CertificatePem": {
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 65536
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CertificateSigningRequest": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "CertificateMode": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "DEFAULT",
            "SNI_ONLY"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Status": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "ACTIVE",
            "INACTIVE",
            "REVOKED",
            "PENDING_TRANSFER",
            "PENDING_ACTIVATION"
          ]
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Id": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    },
    "Arn": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "Status"
  ]
}