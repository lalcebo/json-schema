{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Aws Route53 Recovery Readiness Check Schema and API specification.",
  "taggable": true,
  "definitions": {
    "Tag": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Key": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        },
        "Value": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "$ref": "../component/functions.json#/AwsFunctionString"
            }
          ]
        }
      },
      "required": [
        "Value",
        "Key"
      ]
    }
  },
  "properties": {
    "ResourceSetName": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "[a-zA-Z0-9_]+",
          "minLength": 1,
          "maxLength": 64
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The name of the resource set to check."
    },
    "ReadinessCheckName": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "[a-zA-Z0-9_]+",
          "minLength": 1,
          "maxLength": 64
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "Name of the ReadinessCheck to create."
    },
    "ReadinessCheckArn": {
      "oneOf": [
        {
          "type": "string",
          "maxLength": 256
        },
        {
          "$ref": "../component/functions.json#/AwsFunctionString"
        }
      ],
      "description": "The Amazon Resource Name (ARN) of the readiness check."
    },
    "Tags": {
      "description": "A collection of tags associated with a resource.",
      "type": "array",
      "items": {
        "$ref": "#/definitions/Tag",
        "maxItems": 20
      },
      "insertionOrder": false
    }
  },
  "additionalProperties": false
}