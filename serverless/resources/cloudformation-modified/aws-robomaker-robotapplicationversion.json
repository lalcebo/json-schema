      "permissions": [
        "robomaker:CreateRobotApplicationVersion",
        "s3:GetObject"
      ]
    },
    "delete": {
      "permissions": [
        "robomaker:DeleteRobotApplication",
        "robomaker:DescribeRobotApplication"
      ]
    },
    "read": {
