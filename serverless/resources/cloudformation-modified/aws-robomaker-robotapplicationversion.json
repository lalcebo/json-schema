    "/properties/Arn"
  ],
  "primaryIdentifier": [
    "/properties/Arn"
  ],
  "createOnlyProperties": [
    "/properties/Application",
    "/properties/CurrentRevisionId"
  ],
  "taggable": false,
  "handlers": {
