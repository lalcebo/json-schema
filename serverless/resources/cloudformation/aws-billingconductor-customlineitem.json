        }
      ],
      "title": "InclusiveStartBillingPeriodDefinition"
    },
    "ExclusiveEndBillingPeriod": {
      "oneOf": [
        {
          "type": "string",
          "pattern": "\\d{4}-(0?[1-9]|1[012])"
