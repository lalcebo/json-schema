          ]
        },
        "IamRoles": {
          "type": "array",
          "insertionOrder": false,
          "items": {
            "type": "string",
            "maxLength": 512,
            "minLength": 0
          }
        },
        "LogExports": {
          "type": "array",
          "insertionOrder": false,
          "items": {
            "$ref": "#/definitions/LogExport"
